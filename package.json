{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "description": "Node + TS project boilerplate",
  "main": "index.ts",
  "scripts": {
    "start": "npm run build && node dist/js/index.js",
    "dev": "ts-node-dev --inspect --transpile-only --ignore node_modules --respawn src/index.ts",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricardoalves-dev/ts-boilerplate.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Ricardo J. Alves",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ricardoalves-dev/ts-boilerplate/issues"
  },
  "homepage": "https://github.com/ricardoalves-dev/ts-boilerplate#readme",
  "devDependencies": {
    "@types/node": "^20.11.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
